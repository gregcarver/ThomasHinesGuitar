@model ThomasHinesGuitar.Models.AdminMain
@{
    ViewBag.Title = "AdminMainPage";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<hr />  
<div class="row">
    <ul class="col-lg-2 nav nav-stacked">
        <li class="nav">
            <a href="#">Something</a>
            <br />
            <a href="#">Something Else</a>
        </li>
    </ul>
    <div class="col-lg-10">
        @using (Html.BeginForm("AdminMainPage", "Admin"))
        {
            <h2>AdminMainPage</h2>
            <div class="form-group">
                @Html.LabelFor(m => m.Contact)
                @Html.TextBoxFor(m => m.Contact, new { @class = "form-control" })

            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.Location)
                @Html.TextBoxFor(m => m.Location, new { @class = "form-control" })

            </div>
            <button type="submit" value="Save" class="btn btn-primary">Submit</button>


        }
    </div>
    <div class="row">
        <div class="col-lg-3">
            @Html.DisplayName("Contact")
            @ViewBag.Contact
            <br />
            @Html.DisplayName("Location")
            @ViewBag.Location
        </div>

    </div>
</div>

